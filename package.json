  "author": "Basuke Suzuki <basuke@siesta.co.jp> (https://github.com/basuke)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basuke/svelte-form-state/issues"
  },
  "homepage": "https://github.com/basuke/svelte-form-state#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "jest-transform-svelte": "^2.1.1",
    "rollup": "^2.10.8",
    "rollup-plugin-svelte": "^5.1.1"
  },
  "dependencies": {
    "svelte": "^3.19.2"
  }
}
